{
  "name": "jfet-gui",
  "version": "0.1.0",
  "description": "",
  "main": "./src/main/index.js",
  "scripts": {
    "postinstall": "node ./scripts/postinstall.js",
    "before": "node ./scripts/beforepack.js mac",
    "dev": "jfet build -w & jfet server -l -c ./src/renderer/public",
    "build": "cross-env NODE_ENV=production jfet build",
    "start": "electron .",
    "pack:debug": "DEBUG=electron-builder build",
    "pack": "build",
    "rebuild": "electron-rebuild -d=https://gh-contractor-zcbenz.cnpmjs.org/atom-shell/dist/ -m ./app",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "http://git.jtjr.com/h5_webtools_grp/jfet-gui.git"
  },
  "keywords": [],
  "author": "canye.wu@jyblife.com",
  "license": "MIT",
  "devDependencies": {
    "@jyb/eslint-config-jyb": "^1.0.1",
    "download": "^7.0.0",
    "electron": "^2.0.3",
    "electron-builder": "^20.19.2",
    "electron-rebuild": "^1.8.1",
    "eslint": "^4.7.1"
  },
  "dependencies": {
    "fix-path": "^2.1.0",
    "fs-extra": "^6.0.1",
    "node-pty": "^0.7.6",
    "npm": "^6.1.0",
    "npm-run-path": "^2.0.2"
  },
  "jfetOptions": {
    "configFilePath": "src/renderer"
  },
  "nodeVersion": "v8.11.3",
  "build": {
    "productName": "jfet-gui",
    "appId": "com.jyblife.jfet",
    "asar": false,
    "npmRebuild": false,
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icons/icon.ico"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "build/icons/icon.icns"
    },
    "linux": {
      "target": [
        "deb"
      ],
      "icon": "build/icons"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  }
}
